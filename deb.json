{
  "name": "go-bin-rpm",
  "maintainer": "mh-cbon <mh-cbon@users.noreply.github.com>",
  "description": "Create binary rpm package with ease",
  "changelog-cmd": "changelog debian --vars='{\"name\":\"!name!\"}'",
  "homepage": "http://github.com/mh-cbon/!name!",
  "files": [
    {
      "from": "build/!arch!/!name!",
      "to": "/usr/bin",
      "base" : "build/!arch!/",
      "fperm": "0755"
    }
  ],
